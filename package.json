{
	"name": "rest-api-node-mongodb-redis-jwt",
	"version": "1.0.0",
	"description": " Template REST-API Node.js, Redis, MongoDB, JWT, ES6 REST API",
	"main": "app.js",
	"scripts": {
		"build": "babel src --out-dir build",
		"dev": "nodemon src/app.js --exec babel-node",
		"start": "node build/app.js",
		"create": "babel-node src/firstInit/index.js",
		"remove": "ACTION=remove babel-node src/firstInit/index.js"
	},
	"keywords": [
		"express",
		"mongodb",
		"JWT",
		"Redis",
		"ES6"
	],
	"author": "bondrogee@gmail.com",
	"license": "MIT",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.4.1",
		"express": "4.19.2",
		"express-validator": "^7.0.1",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.1.1",
		"morgan": "^1.10.0",
		"qs": "^6.12.0",
		"redis": "^4.6.12"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.9",
		"@babel/core": "^7.23.9",
		"@babel/node": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"nodemon": "^3.0.3",
		"prettier": "^3.2.4",
		"prettier-eslint": "^16.3.0"
	}
}
